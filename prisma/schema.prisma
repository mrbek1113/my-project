generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int @id @default(autoincrement())
first_name String?
last_name String?
phone     String?
user_name String @unique
password  String
created_at DateTime @default(now())
updated_at DateTime @updatedAt
role Role
}


enum Role { 
  student
  mentor_id
  admin
  big_admin
}
